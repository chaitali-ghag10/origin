
public class Demo2d {

	public static void main(String[] args) {
		int[][] arr = new int[3][3];
		
		arr[0][0] = 1;
		arr[0][1] = 1;
		arr[0][2] = 1;
		
		arr[1][0] = 1;
		arr[1][1] = 1;
		arr[1][2] = 1;
		
		arr[2][0] = 1;
		arr[2][1] = 1;
		arr[2][2] = 1;
		
		for(int i=0;i<3;i++) {
			for(int j=0;j<3;j++) {
				System.out.print(arr+"\t");
			}
			System.out.println();
		}
		
		
	}

}
