	public static void main(String[] args) {
		Student st = new Student(23, 450, "abc");
		
		try {
			FileOutputStream fo = new FileOutputStream("Student.txt");
		} catch (FileNotFoundException e) {
			// TODO Auto-generated catch block
			e.printStackTrace();
		}
		
	}

}
