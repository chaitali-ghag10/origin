
public class Person {
	
	private int age;
	private String name;
	
	
	public void display() {
		System.out.println("Name:"+name);
		System.out.println("Age:"+age);
	}
	
	public void setAge(int a) {
		age =a;
	}
	
	public void setName(String n) {
		name=n;
	}
	
	public int getAge() {
		return age;
	}
	
	public String getName()
	{
		return name;
	}
	

}
