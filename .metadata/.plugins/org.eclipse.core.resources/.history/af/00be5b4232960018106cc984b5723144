
public class Person {
	
	private int age;
	private String name;
	
	public Person() {
		age=10;
		name = "ABC";
	}
	
	public Person(int a, String n) {
		age=a;
		name=n;
	}
	
	public void display() {
		System.out.println("Name:"+name);
		System.out.println("Age:"+age);
}
