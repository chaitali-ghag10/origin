
public class DemoJagged {

	public static void main(String[] args) {
		int [][] values = new int [3][];
		
		values[0] = new int[4];
		values[0][0] = 122;
		values[0][1] = 123;
		values[0][2] = 124;
		values[0][3] = 125;
		
		values[1] = new int[2];
		values[1][0] = 122;
		values[1][1] = 122;
		
		values[2] = new int[1];
		values[2][0] = 122;
		
		for(int i=0;i<values.length;i++) {
			for(int j=0;j<values[i].length;j++) {
				System.out.print(values[i][j]+"\t");
			}
			System.out.println();
		}
		

	}

}
