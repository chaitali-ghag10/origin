
public class StaticPrac {
	
	private float age;
	private String name;
	
	private static int value;
	
	public StaticPrac() {
		age=12.12f;
		name="abcd";
		value++;
	}

	static {
		value =0;
	}
	
	public void display() {
		System.out.println("Name:"+name);
		System.out.println("Age:"+age);
		System.out.println("Value:"+value);
	}
	
	public static void show() {
		System.out.println("value:"+value);
	}
	
	public static void main(String[] args) {
		StaticPrac.show();
		
		StaticPrac s=new StaticPrac();
		StaticPrac s1=new StaticPrac();
		StaticPrac s2=new StaticPrac();
		StaticPrac s3=new StaticPrac();
		StaticPrac s4=new StaticPrac();

	}

}
